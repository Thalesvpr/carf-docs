{
  "name": "@carf/tscore",
  "version": "0.1.0",
  "description": "TypeScript shared library for CARF ecosystem - Auth, Validations, Domain Types",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./auth/react": "./dist/auth/react/index.js",
    "./auth/vue": "./dist/auth/vue/index.js",
    "./validations": "./dist/validations/index.js",
    "./types": "./dist/types/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir ./dist",
    "build:all": "bun run build && bun run build:types",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "prepublishOnly": "bun run build:all"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0",
    "vue": "^3.4.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Thalesvpr/carf-tscore.git"
  },
  "keywords": [
    "carf",
    "typescript",
    "validation",
    "keycloak",
    "oauth2",
    "cpf",
    "cnpj",
    "brasil"
  ],
  "author": "CARF Team",
  "license": "UNLICENSED"
}
