# Development Environment Variables
# IMPORTANT: These are INSECURE values for local development only
# Never use these values in staging or production environments

# PostgreSQL
POSTGRES_DB=carf_dev
POSTGRES_USER=dev_user
POSTGRES_PASSWORD=dev_password_insecure_change_me

# Keycloak
KEYCLOAK_DB=keycloak_dev
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin_insecure_change_me
KEYCLOAK_HOSTNAME=localhost

# RabbitMQ
RABBITMQ_USER=guest
RABBITMQ_PASS=guest

# JWT Secret (256-bit base64 encoded)
# Generated with: openssl rand -base64 32
JWT_SECRET=dev-secret-256-bit-key-change-in-production-base64

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Sentry (optional for development)
SENTRY_DSN=
SENTRY_DSN_FRONTEND=

# Google Analytics (optional for development)
GA_ID=

# Log Level
LOG_LEVEL=debug

# Feature Flags
ENABLE_SWAGGER=true
ENABLE_AUDIT_LOG=false
FEATURE_FLAG_OFFLINE_SYNC=true

# File Upload
FILE_UPLOAD_MAX_SIZE_MB=10

# External APIs (optional - leave empty to use defaults or mocks)
GEOCODING_API_URL=https://nominatim.openstreetmap.org
GEOCODING_API_KEY=
SERPRO_CPF_API_URL=
SERPRO_CLIENT_ID=
SERPRO_CLIENT_SECRET=

# SMTP (optional - leave empty to use console logging)
SMTP_HOST=
SMTP_PORT=587
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_FROM_EMAIL=noreply@localhost

# S3/Storage (optional - leave empty to use local filesystem)
S3_BUCKET_NAME=
S3_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Redis
REDIS_PASSWORD=

# Timeouts
DATABASE_COMMAND_TIMEOUT=30
API_TIMEOUT_MS=30000

# Session
SESSION_TIMEOUT_MINUTES=30

# Development-specific
ASPNETCORE_ENVIRONMENT=Development
NODE_ENV=development
