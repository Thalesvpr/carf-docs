{
  "clientId": "geoweb",
  "name": "GEOWEB - Frontend React",
  "description": "Aplicação web de cadastramento de unidades habitacionais",
  "enabled": true,
  "publicClient": true,
  "standardFlowEnabled": true,
  "implicitFlowEnabled": false,
  "directAccessGrantsEnabled": false,
  "redirectUris": [
    "http://localhost:5173/*",
    "http://localhost:3000/*",
    "https://geoweb.carf.example.com/*"
  ],
  "webOrigins": ["+"],
  "attributes": {
    "pkce.code.challenge.method": "S256",
    "post.logout.redirect.uris": "+"
  },
  "defaultClientScopes": ["openid", "profile", "email", "roles", "carf-tenant"],
  "optionalClientScopes": ["offline_access"]
}
